{
  "title": "Change caps_lock key",
  "rules": [
    {
      "description": "Change caps_lock to tab when combined with left Command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": "left_command"
            }
          ]
        }
      ]
    }
  ]
}
